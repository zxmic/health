<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="po.entity.MypointEntity" table="mypoint" schema="jspssh">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="studentid">
            <column name="studentid" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="moodpoint">
            <column name="moodpoint" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="whichday">
            <column name="whichday" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>