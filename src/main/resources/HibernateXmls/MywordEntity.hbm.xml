<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="po.entity.student.MywordEntity" table="myword" schema="jspssh">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="studentid">
            <column name="studentid" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="todaymyword">
            <column name="todaymyword" sql-type="varchar(200)" length="200" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>